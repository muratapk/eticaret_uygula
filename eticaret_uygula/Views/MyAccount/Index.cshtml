@model eticaret_uygula.Dto.AppUserEditDto
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<div class="row">
    <div class="col-md-8">
        <form asp-action="Index"  asp-controller="MyAccount">
        <div class="form-group">
            <input asp-for="FirstName" placeholder=" Adını Girin" class="form-control" />
        </div>
         <div class="form-group">
            <input asp-for="LastName" placeholder=" Soyadını Girin" class="form-control" />
        </div>
            <div class="form-group">
                <input asp-for="Email" placeholder="Email Adresiniz Girin" class="form-control" />
            </div>
            <div class="form-group">
                <input asp-for="City" placeholder="Şehir Adresiniz Girin" class="form-control" />
            </div>
            <div class="form-group">
                <input asp-for="Password" placeholder="Şifrenizi Girin" class="form-control" />
            </div>
            <div class="form-group">
                <input asp-for="ConfirmPassword" placeholder="Şifrenizi Tekrar Girin" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Düzelt" class="btn btn-primary btn-lg" />
            </div>
        </form>
    </div>
</div>